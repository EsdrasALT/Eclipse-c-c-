int iterativa(int numero, int multiplicador){
	int resultado=0;
	for (int i=0;i<multiplicador;i++){
		resultado = resultado + numero;

	}
	return resultado;
}

int recursiva(int numero, int multiplicador){
	if (multiplicador > 0){
		return numero + recursiva(numero, multiplicador-1);
	}
	return 0;
}
