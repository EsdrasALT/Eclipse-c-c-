		return acumulador;
	}
	return multiplicacao_recursiva(a, b - 1, acumulador + a);
}

int multiplicacao_recursiva(int a, int b, int acumulador) {
    if (b == 0) {
        return acumulador;
    }
    return multiplicacao_recursiva(a, b - 1, acumulador + a);
}

