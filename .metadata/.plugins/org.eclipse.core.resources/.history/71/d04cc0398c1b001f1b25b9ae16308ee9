int main (void){
	setbuf(stdout,NULL);

	int numero=5, contador=6;

	printf("---INICIO----\n");
	iterativo(numero,contador);
	printf("----FIM-----\n");

	printf("---INICIO----\n");
	recursivaComum(numero,contador);
	printf("----FIM-----\n");

	printf("---INICIO----\n");
	recursivaDeCauda(numero,contador);
	printf("----FIM-----\n");

	return 0;
}

