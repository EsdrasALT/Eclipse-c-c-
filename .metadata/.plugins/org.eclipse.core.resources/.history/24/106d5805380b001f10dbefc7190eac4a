#include <stdio.h>
#include <string.h>

struct st_vetor {
	float x;
	float y;
	float z;

}Vetor;

int main (){
	setbuf(stdout,NULL);

	printf("Digite os valores de x,y e z, respectivamente: ");
	scanf("&2f, &2f, &2f",&Vetor.x, &Vetor.y, &Vetor.z);

	printf("A soma dos valores x e y", (Vetor.x + Vetor.y));

	return 0;
}
