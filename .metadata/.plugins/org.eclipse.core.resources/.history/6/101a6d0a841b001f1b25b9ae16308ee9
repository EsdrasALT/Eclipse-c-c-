void iterativo(int numero, int contador){
	int total=0;
	for(int i=1;i<=contador;i++){
		total = total + numero;
		printf("%d° - %d x %d = %d\n",i,numero,i,total);
	}
}

int recursivaComum(int numero, int contador){
	printf("%d° - %d x %d = %d\n",contador,numero,contador,numero);
	if (contador > ){
		return numero + recursivaComum(numero, contador-1);
	}
	return 0;
}
